#container
  #header
    %h1=link_to APP_CONFIG[:app_name], home_path
  .clearfix
    = render 'application/nav'
    = yield(:pre_content)
    .content
      - flash.each do |name, msg|
        = content_tag :div, msg, :id => "flash_#{name}"
      = yield
    = yield(:post_content)
  #overlay.overlay
    .contentWrap
  = render 'application/footer'
